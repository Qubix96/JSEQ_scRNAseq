#!/bin/bash

function seurat_start {
echo 'Start seurat analysis'

seurat_data=$(pwd)/projects/$project_name_mode/
seurat_out=$(pwd)/projects/$project_name_mode/results
PATH_MARKERS=$(pwd)/requirements_file/markers.xlsx

SCRIPTS_EXTRACT=$(pwd)/scripts
Rscript $SCRIPTS_EXTRACT/seurat_cluster.R $seurat_data $seurat_out $cell $project_name_mode $species $PATH_MARKERS

}